/**app.wxss**/
@import './vendor/iconfont.wxss';
page {
  --baseColor: #ff2d4a;
}
page,view,text,swiper,swiper-item,image,navigator {

  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

image {
  vertical-align: middle;
}
/* 文字显示2行 */
.line2 {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}